# Solana Network Configuration
SOLANA_RPC_URL=https://api.devnet.solana.com
SOLANA_WS_URL=wss://api.devnet.solana.com

# Wallet Configuration
KEYPAIR_PATH=/path/to/your/keypair.json

# Trading Parameters
MIN_PROFIT_PERCENTAGE=1.0
MAX_TRADE_SIZE=1000000000
DAILY_VOLUME_LIMIT=10000000000

# Strategy Configuration
USE_FLASH_LOANS=true
FLASH_LOAN_SOURCES=solend,port,marinade
EXECUTION_STRATEGIES=jit,flash_loan,front_running

# Security Settings
MEV_PROTECTION=true
QUANTUM_SECURITY=true
SECURITY_LEVEL=high
MAX_SLIPPAGE=1.0
POSITION_TIMEOUT=30000

# Market Configuration
WHITELISTED_MARKETS=market1,market2,market3
WHITELISTED_TOKENS=token1,token2,token3
MIN_LIQUIDITY=1000000
MAX_SPREAD=0.05

# Performance Settings
MAX_CONCURRENT_TRADES=3
METRICS_ENABLED=true
PERFORMANCE_TRACKING=true

# Monitoring Configuration
LOG_LEVEL=info
ALERT_ENDPOINTS=http://your-monitoring-service/webhook

# Network Settings
MAX_RETRIES=3
TIMEOUT_MS=5000
REQUIRE_CONFIRMATIONS=1

# Development Mode
DEV_MODE=true
SIMULATE_TRADES=false
